<script type='module' is:inline>
	window.addEventListener('load', () => {
		const paths = Array.from(document.querySelectorAll('#nameDevDouble path'));
		console.log(paths);
		const duration = 0.3; // duración por letra en segundos
		let forward = true;

		const animateStroke = () => {
			// Define orden según dirección
			const orderedPaths = forward ? paths : [...paths].reverse();

			// Prepara paths
			for (const path of paths) {
				const length = path.getTotalLength();
				path.style.strokeDasharray = length;
				path.style.strokeDashoffset = forward ? length : 0;
				path.style.transition = 'none'; // reset rápido
			}

			// Ejecuta animación en orden
			setTimeout(() => {
				for (const [i, path] of orderedPaths.entries()) {
					const length = path.getTotalLength();
					const delay = i * duration * 500;
					setTimeout(() => {
						path.style.transition = `stroke-dashoffset ${duration}s ease-in-out`;
						path.style.strokeDashoffset = forward ? '0' : length;
					}, delay);
				}
			}, 50);

			// Cambia dirección al finalizar
			const totalDuration = paths.length * duration * 500 + 300;
			setTimeout(() => {
				forward = !forward;
				animateStroke();
			}, totalDuration);
		};

		animateStroke();
	});
</script>
